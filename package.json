{
  "name": "murilo-zc.github.io",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "npx expo export --platform web --clear",
    "build:web": "npx expo export --platform web --clear",
    "deploy": "npx expo export --platform web --clear && touch dist/.nojekyll && cp CNAME dist/",
    "deploy:local": "bash scripts/deploy.sh",
    "serve": "npx serve dist",
    "preview": "npm run build:web && npm run serve"
  },
  "dependencies": {
    "expo": "~53.0.13",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "expo-router": "~5.1.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "expo-linking": "~7.1.5",
    "expo-constants": "~17.1.6",
    "react-native-web": "^0.20.0",
    "react-dom": "19.0.0",
    "expo-splash-screen": "~0.30.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
